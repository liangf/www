<?xml version="1.0" encoding="iso-8859-1"?>
<xs:schema id="Products" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
	<xs:element name="Products" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">		
				<xs:element name="Product">
         			<xs:complexType>
         				<xs:sequence>
         					<xs:element msprop:TableNameXsd="Products" msprop:IsIdentity="true" name="link" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="0" maxOccurs="1"/>
							
         			        <xs:element name="title" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
							<xs:element name="description" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="0" maxOccurs="1"/> 
							<xs:element name="image_link" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
							<xs:element name="price"   msprop:SqlDbType="Money" type="xs:decimal" minOccurs="0" maxOccurs="1" />
							<xs:element  name="id"  msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="1" maxOccurs="1"/>
							<xs:element name="Shipping"   type="xs:string" minOccurs="0" maxOccurs="1"/>							 
							<xs:element name="quantity"   msprop:SqlDbType="Int" type="xs:int" minOccurs="0" maxOccurs="1"/>
							<xs:element name="model_number" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="0" maxOccurs="1"/>
							<xs:element name="gtin" msprop:maxLength="255" msprop:SqlDbType="VarChar" type="xs:string" minOccurs="0" maxOccurs="1"/>
							 
					        <xs:element name="google_product_category" msprop:maxLength="255"  type="xs:string" minOccurs="0" maxOccurs="1"/>
							
							
							
							
			    		</xs:sequence>  
					</xs:complexType>	
				</xs:element>
			</xs:choice>
		</xs:complexType>		
	</xs:element>
</xs:schema>